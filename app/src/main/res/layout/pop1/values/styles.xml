<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="mProgress_circle">
        <item name="android:indeterminateDrawable">@drawable/progress_loading</item>
        <item name="android:minWidth">25dp</item>
        <item name="android:minHeight">25dp</item>
        <item name="android:maxWidth">60dp</item>
        <item name="android:maxHeight">60dp</item>
    </style>

    <style name="popwin_from_bottom_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_from_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/pop_from_bottom_out</item>
    </style>

    <style name="popwin_from_top_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_from_top_in</item>
        <item name="android:windowExitAnimation">@anim/pop_from_top_out</item>
    </style>

    <style name="popwin_from_left_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_from_left_in</item>
        <item name="android:windowExitAnimation">@anim/pop_from_left_out</item>
    </style>


    <style name="popwin_from_right_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_from_right_in</item>
        <item name="android:windowExitAnimation">@anim/pop_from_right_out</item>
    </style>
</resources>